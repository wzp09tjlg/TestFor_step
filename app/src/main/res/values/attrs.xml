<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--gridview相关-->
    <declare-styleable name="lbBaseGridView">
        <attr name="focusOutFront" format="boolean" />
        <attr name="focusOutEnd" format="boolean" />
        <attr name="horizontalMargin" format="dimension" />
        <attr name="verticalMargin" format="dimension" />
        <attr name="android:gravity" />
    </declare-styleable>
    <declare-styleable name="lbHorizontalGridView">
        <attr name="rowHeight" format="dimension">
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="numberOfRows" format="integer" />
    </declare-styleable>
    <declare-styleable name="lbVerticalGridView">
        <attr name="columnWidth" format="dimension">
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="numberOfColumns" format="integer" />
    </declare-styleable>
    <!--浮层相关-->
    <declare-styleable name="FancyCoverFlow">
        <attr name="unselectedAlpha" format="float"/>
        <attr name="unselectedSaturation" format="float"/>
        <attr name="unselectedScale" format="float"/>
        <attr name="maxRotation" format="integer"/>
        <attr name="scaleDownGravity" format="float"/>
        <attr name="actionDistance" format="integer">
            <enum name="auto" value="2147483647" />
        </attr>
    </declare-styleable>
    <!--浮层其他设置相关-->
    <!--FocusZoomUtil相关-->
    <item name="lb_focus_animator" type="id" />
    <item name="lb_focus_zoom_factor_large" type="fraction">150%</item>
    <item name="lb_focus_zoom_factor_llarge" type="fraction">140%</item>
    <item name="lb_focus_zoom_factor_medium" type="fraction">130%</item>
    <item name="lb_focus_zoom_factor_small" type="fraction">110%</item>
</resources>